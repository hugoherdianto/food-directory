<%= form_for @user do |f| %>
  <p>
  <%= f.label :name %>
  <%= f.text_field :name %>
  </p>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <p>
  <%= f.label :password %>
  <%= f.password_field :password %>
  </p>

  <%= f.submit 'Create' %>
<% end %>

<p><%= link_to '<-- Go Back', login_path %></p>
